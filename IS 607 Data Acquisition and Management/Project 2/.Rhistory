pythag <- function(f,g,h){
#This function takes in three numbers: f, g, and h.
#It returns TRUE if the numbers form a pythagorean triple, and FALSE if they don't.
if(f^2+g^2==h^2 | f^2+h^2==g^2 | g^2+h^2==f^2){
return(TRUE)
} else {
return(FALSE)
}
}
